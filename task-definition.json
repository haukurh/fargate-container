{
    "family": "fargate-container",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "fargate-container",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
