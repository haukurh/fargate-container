{
    "family": "fargate-container",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::398808102714:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "fargate-container",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
